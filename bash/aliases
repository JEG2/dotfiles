# MySQL
alias start_mysql="sudo launchctl load -w /Library/LaunchDaemons/com.mysql.mysqld.plist"

# Git
alias g="git status"
alias ga="git add"
alias gaa="git add ."
alias gc="git commit -m"
alias gca="git commit -am"
alias gb="git branch"
alias gco="git checkout"
alias gl="git log"
alias gs="git show"
alias gd="git diff"
alias gbl="git blame"
alias gps="git push"
alias gpl="git pull"

# Ruby
alias r="ruby"
alias i="irb"
alias r_="ruby_dev"
alias i_="irb_dev"
alias ri_="ri_dev"
alias rk="rake"
alias rk_="rake_dev"
alias rk_t="rake_dev -T"

# Rails
alias sc="script/console"
alias ss="script/server"
alias rkt="rake test"
alias rkdm="rake db:migrate"
